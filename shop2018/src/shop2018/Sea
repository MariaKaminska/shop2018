
package RiversAndSeas;

/**
 *
 * @author Maria Kami≈Ñska
 */
public class Sea {
    
    private String name;
    private Double area;
    
    public Sea(String name, Double area)
    {
        this.name=name;
        this.area=area;
    }
    
    @Override
    
    public String toString(){
        
        return name+" - "+area+" square kilometres";
    }
    public boolean equals(Sea sea){
        
        return name.equals(sea.name);
    }
    public Double getArea(){
        
        return area;
    }
    
}
